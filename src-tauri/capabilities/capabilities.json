{
    "$schema": "https://schema.tauri.app/config/2.0.0",
    "identifier": "default",
    "description": "Default capabilities for GNS Browser",
    "windows": [
        "main"
    ],
    "permissions": [
        "core:default",
        "core:event:default",
        "core:event:allow-listen",
        "core:event:allow-emit",
        "shell:allow-open",
        "http:default",
        "os:default",
        "notification:default",
        "dialog:default"
    ]
}